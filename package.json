{
    "name": "nextjs-firebase-express-starter",
    "version": "1.0.0",
    "description": "NextJS, Firebase, ExpressJS Starter Stack",
    "main": "index.js",
    "scripts": {
        "build:server": "tsc --project tsconfig.server.json",
        "build:next": "next build",
        "build": "npm run build:next && npm run build:server",
        "prod:windows": "set NODE_ENV=production&& node dist/index.js",
        "prod:mac": "NODE_ENV=production node dist/index.js",
        "start": "npm run build && npm run prod:windows"
    },
    "keywords": [
        "nextjs",
        "firebase",
        "expressjs"
    ],
    "author": "Errol Edric Dee",
    "license": "ISC",
    "dependencies": {
        "express": "^4.17.3",
        "next": "^9.3.6",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/node": "^13.13.4",
        "@types/react": "^16.9.34",
        "ts-node": "^8.9.1",
        "typescript": "^3.8.3"
    }
}
